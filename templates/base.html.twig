<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <nav style="display: flex; gap: 1em">
            <a href="{{ path('app_voiture_catalogue') }}">Catalogue</a>
            <a href="{{ path('app_voiture_index') }}">Voitures</a>
            <a href="{{ path('app_garage_index') }}">Garages</a>
            <a href="{{ path('app_lieu_index') }}">Lieux</a>
            <a href="{{ path('app_critere_index') }}">Critères</a>
            <a href="{{ path('app_user_index') }}">Users</a>
        </nav>
        {% block body %}{% endblock %}
    </body>
</html>
